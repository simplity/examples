<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="jmsListener"
	moduleName="jms"
	dbAccessType="none"
	jmsUsage="notManaged">
	<actions>
		<jmsConsumer waitForMessage="true" consumeAll="true">
			<requestDestination name="dynamicQueues/TTReadQQ" messageBodyType="text">
				<inputData>				
					<inputRecords>
						<inputRecord recordName="ticket" />
						<inputRecord recordName="relatedObjects" />
						<inputRecord recordName="relatedParties" />
						<inputRecord recordName="notes" />
					</inputRecords>
				</inputData>			
			</requestDestination>
			<actions>
				<subService serviceName="troubleTicketUpdate"/>
			</actions>
		</jmsConsumer>
	</actions>
</service>