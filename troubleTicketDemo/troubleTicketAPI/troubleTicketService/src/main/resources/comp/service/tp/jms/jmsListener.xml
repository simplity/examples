<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="jmsListener"
	moduleName="jms"
	dbAccessType="none"
	jmsUsage="notManaged">
	<inputData>
		<inputRecords>
			<inputRecord recordName="ticket" />
		</inputRecords>
	</inputData>
	<actions>
		<jmsConsumer waitForMessage="true" consumeAll="true">
			<requestQueue queueName="dynamicQueues/TTReadQQ" recordName="ticket"/>
			<actions>
				<log names="id"></log>
			</actions>
		</jmsConsumer>
	</actions>
</service>