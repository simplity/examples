<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="jmsListener"
	moduleName="jms"
	dbAccessType="none"
	jmsUsage="notManaged">
	<actions>
		<jmsConsumer waitForMessage="true" consumeAll="true">
			<requestDestination name="dynamicQueues/TTReadQQ" recordName="ticket"/>
			<actions>
				<log names="id"></log>
				<subService serviceName="troubleTicketUpdate"/>
			</actions>
		</jmsConsumer>
	</actions>
</service>