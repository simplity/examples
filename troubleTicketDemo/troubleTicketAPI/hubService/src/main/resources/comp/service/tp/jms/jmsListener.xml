<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="jmsListener"
	moduleName="jms"
	dbAccessType="none"
	jmsUsage="notManaged">
	<inputData>
		<inputRecords>
			<inputRecord recordName="jmsTicket" />
		</inputRecords>
	</inputData>
	<actions>
		<jmsConsumer waitForMessage="true" consumeAll="true">
			<requestDestination name="dynamicQueues/TTWriteQQ"  recordName="jmsTicket"/>
			<actions>
				<subService serviceName="publish.processMessages"/>
			</actions>
		</jmsConsumer>
	</actions>
</service>