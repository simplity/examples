<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="contractGet"
	dbAccessType="readWrite">
	<inputData justInputEveryThing="true">
		<inputFields>
			<inputField dataType="_number" name="contractId" />
		</inputFields>
	</inputData>
	<actions>
		<createSheet sheetName="filterSheet" data="id;$contractId" />
		<filter filterRecordName="Contract" inputSheetName="filterSheet">
			<childRecords>			
				<relatedRecord sheetName="storage" recordName="Storage"/>
				<relatedRecord sheetName="roles" recordName="Role"/>
			</childRecords>
		</filter>
		<filter filterRecordName="RoleType" outputSheetName="roleType"> 
		</filter>
		<log names="country,city,county,throughputsPerYear,econsUOM,leaseType,segment,id,state,contractingEntity,dealCounterParty,durationInMonths,bench,otherRefNum,contractStartDate,contractEndDate,excessThroughputRateUOM,terminal,econs,dealName,assetOwner,contractSignDate,assetName,contractNum,region,excessThroughputRate,desc,status,notes,storage,roles,roleType"/>
	</actions>
	<outputData justOutputEveryThing="true" fieldNames="country,city,county,throughputsPerYear,econsUOM,leaseType,segment,id,state,contractingEntity,dealCounterParty,durationInMonths,bench,otherRefNum,contractStartDate,contractEndDate,excessThroughputRateUOM,terminal,econs,dealName,assetOwner,contractSignDate,assetName,contractNum,region,excessThroughputRate,desc,status,notes">
		<outputRecords>
			<outputRecord sheetName="storage" />
			<outputRecord sheetName="roles" />
			<outputRecord sheetName="roleType" parentSheetName="roles" linkColumnInParentSheet="roleId" linkColumnInThisSheet="roleId"/>
		</outputRecords>
	</outputData>
</service>