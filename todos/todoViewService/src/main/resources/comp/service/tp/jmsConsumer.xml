<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service xmlns="http://www.simplity.org/schema" 
	name="jmsConsumer"
	dbAccessType="none" 
	jmsUsage="notManaged">

	<actions>
		<jmsConsumer waitForMessage="false" consumeAll="false">
			<requestQueue queueName="dynamicQueues/Queue01" extractAll="false" bodyFieldName="queueMessage" messageBodyType="text"/>
			<actions>
				<log executeOnCondition="queueMessage" names="queueMessage" />
			</actions>
		</jmsConsumer>
	</actions>
	<outputData fieldNames="queueMessage">
	</outputData>
</service>